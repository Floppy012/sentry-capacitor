{
  "name": "@sentry/capacitor",
  "version": "0.0.1",
  "description": "Sentry SDK for Capacitor",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Sentry Team and Contributors",
  "license": "MIT",
  "dependencies": {
    "@sentry/browser": "^5.27.3",
    "@sentry/core": "^5.27.3",
    "@sentry/hub": "^5.27.3",
    "@sentry/integrations": "^5.27.3",
    "@sentry/tracing": "^5.27.3",
    "@sentry/types": "^5.27.3",
    "@sentry/utils": "^5.27.3",
    "@sentry/wizard": "^1.1.4",
    "promise": "^8.1.0"
  },
  "devDependencies": {
    "@capacitor/android": "^2.4.2",
    "@capacitor/core": "^2.4.2",
    "@capacitor/ios": "^2.4.2",
    "@ionic/prettier-config": "^1.0.0",
    "@ionic/swiftlint-config": "^1.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@sentry-internal/eslint-config-sdk": "^5.27.3",
    "@sentry-internal/eslint-plugin-sdk": "^5.27.3",
    "@sentry/typescript": "^5.20.1",
    "eslint": "^7.13.0",
    "prettier": "^2.1.2",
    "prettier-plugin-java": "^0.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.21.0",
    "swiftlint": "^1.0.1",
    "typescript": "^4.0.5",
    "@types/jest": "^26.0.15",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.2"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "SentryCapacitor.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/getsentry/sentry-capacitor"
  },
  "bugs": {
    "url": "https://github.com/getsentry/sentry-capacitor/issues"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "globals": {
      "__DEV__": true,
      "ts-jest": {
        "tsconfig": "./tsconfig.json",
        "diagnostics": false
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/sample/"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.(ts|tsx)"
    ]
  }
}
